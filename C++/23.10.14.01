#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
int main()
{
    int a;
    printf("put your grade");
    scanf("%d", &a);

    if (a <= 0 || a >= 100)
    {
        printf("error");
        return 0;
    }
    
    switch (a/10)
    {
        case 10:
        case 9:
            printf("A");
            break;
        case 8:
            printf("B");
            break;

        case 7:
            printf("C");
            break;
        case 6:
            printf("D");
            break;
        default:
            printf("E");
    }

    return 0;
}
